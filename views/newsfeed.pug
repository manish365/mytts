extends layouts/layout
block content
  include layouts/header
  div.container.main-content
    .row 
      .column
        include layouts/breadcrumb
    each item, index in data
      .row 
        .column.mb-2
          a.link-card(href=`/news-feed/${item['name']}/${index}`) 
            span.capitalize  #{item['name']} #{item['categoryid']} (#{item['language']})
